  "codeActions": [
    {
      "id": "3adb5f3c-c9f6-cb6d-e5af-210f805aa7fc",
      "name": "UserWithRoles",
      "comment": "Returns users with the Title field - Name (Role1, Role2, ...)",
      "definedOnServer": false,
      "type": 1,
      "source": "var users = await OptimaJet.DWKit.Core.Metadata.DbObjects.SecurityUser.SelectAsync(Filter.Empty, Order.StartAsc(\"Name\"));\n\nvar res = new List<DynamicEntity>(users.Count);\nforeach(var user in users)\n{\n    var item = user.AsDynamicEntity;\n    item.Title = string.Format(\"{0} ({1})\", user.Name, string.Join(\", \", (await user.Roles.AsyncValue)));\n    res.Add(item);\n}\nreturn res;",
      "usings": "System;\nSystem.Collections;\nSystem.Collections.Generic;\nSystem.Linq;\nSystem.Threading;\nSystem.Threading.Tasks;\nOptimaJet.Workflow;\nOptimaJet.Workflow.Core.Runtime;\nOptimaJet.Workflow.Core.Model;\nOptimaJet.DWKit.Core;\nOptimaJet.DWKit.Core.Model;",
      "isAsync": true
    }
